    white(Up 0 1, Up 1 2, Up 2 1, Up 1 0) and blocks([Front 0 1, Front 1 1], [Left 0 1, Left 1 1], [Back 0 1, Back 1 1], [Right 0 1, Right 1 1])
    orientation(Up, Front) and orientation(Up, Right) and orientation(Up, Back) and orientation(Up, Left)
